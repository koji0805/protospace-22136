<main class="main">
  <div class="inner">
    <% if user_signed_in? %>
      <div class="greeting">
        こんにちは、
        <%= link_to "#{current_user.name}さん",  class: :greeting__link%>
      　　　　　　　　　　　　　　　　　　　　　　<%# ↑user_path(current_user.id),を後ほど追加する %>
      </div>
    <% end %>
    <div class="card__wrapper">
      <%= render partial: 'prototype', collection: @prototypes %>
    </div>
  </div>
</main>
